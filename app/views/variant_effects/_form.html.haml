= form_for @variant_effect do |f|
  - if @variant_effect.errors.any?
    #error_explanation
      %h2= "#{pluralize(@variant_effect.errors.count, "error")} prohibited this variant_effect from being saved:"
      %ul
        - @variant_effect.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :snp
    = f.text_field :snp
  .field
    = f.label :variant
    = f.text_field :variant
  .field
    = f.label :description
    = f.text_field :description
  .actions
    = f.submit 'Save'
